// for counting terminal
#include <stdio.h>
#include <string.h>
#include <ctype.h>
int main(){
    char a[100];
    int j,c=0,d=0;
    fgets(a,sizeof(a),stdin);
    int len=strlen(a);
    a[len-1]=='\0';
    int flag[256]={0};
    for(int i=0;i<len-1;i++){
        if(isupper(a[i])||a[i]=='-'||a[i]=='>'||a[i]=='/'||a[i]==' '){
                d++;
        }
        else{
            j=(int)a[i];
                flag[j]=1;
        }
    }
    for(int j=0;j<256;j++){
        if(flag[j]==1){
        c++;}
    }
    printf("the number of  terminals are %d",c);
}



// for counting non terminal
#include <stdio.h>
#include <string.h>
#include <ctype.h>
int main(){
    char a[100];
    int c=0;
    scanf("%s",a);
    int len=strlen(a);
    for(int i=0;i<len;i++){
        if(isupper(a[i])){
            c++;
        }
    }
    printf("the number of non terminals are %d",c-1);
}


// for counting null
#include <stdio.h>
#include <string.h>
#include <ctype.h>
int main(){
    char a[100];
    int c=0;
    scanf("%s",a);
    int len=strlen(a);
    for(int i=0;i<len;i++){
        if(a[i]=='#'){
            c++;
        }
    }
    printf("the number of NULL productions  are %d",c);
}
