// for counting non terminal
#include <stdio.h>
#include <string.h>
#include <ctype.h>
int main(){
    char a[100];
    int c=0;
    scanf("%s",a);
    int len=strlen(a);
    for(int i=0;i<len;i++){
        if(isupper(a[i])||a[i]=='-'||a[i]=='>'||a[i]=='/'){
            c++;
        }
    }
    printf("the number of non terminals are %d",len-c);
}
